{
  "projectSettings": {
    "framework": null
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "builds": [
    {
      "src": "server/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "server",
  "routes": [
    { "src": "/api/(.*)", "dest": "server/api/$1.js" }
  ]
}
